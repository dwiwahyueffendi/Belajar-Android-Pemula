<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.submisisionandroidpemula">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission-sdk-23 android:name="android.permission.INTERNET"></uses-permission-sdk-23>

    <application
        android:allowBackup="true"
        android:icon="@drawable/singing"
        android:label="@string/app_name"
        android:roundIcon="@drawable/singing"
        android:supportsRtl="true"
        android:theme="@style/Theme.SubmisisionAndroidPemula">
        <activity android:name=".AboutActivity" />
        <activity android:name=".OnClickActivity"/>
        <activity android:name=".SplashScreen" android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>